    "@types/jest": "24.0.16",
    "@types/jsonwebtoken": "8.3.2",
    "@types/lodash": "4.14.136",
    "@types/mongoose": "5.5.11",
    "@types/webpack-env": "1.14.0",
    "apollo-server-testing": "2.8.1",
    "clean-webpack-plugin": "3.0.0",
    "coveralls": "3.0.5",
