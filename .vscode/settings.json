{
  "editor.formatOnSave": true,
  "prettier.eslintIntegration": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true
  },
  "cSpell.words": [
    "typegoose",
    "virtuals"
  ]
}
