{
  "editor.formatOnSave": true,
  "prettier.eslintIntegration": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true
  },
  "cSpell.words": [
    "Không",
    "khoản",
    "khẩu",
    "mockingoose",
    "thấy",
    "typegoose",
    "virtuals",
    "đúng"
  ]
}
